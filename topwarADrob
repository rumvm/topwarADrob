Sub EERhandle	
	
	Dim ErH_timer
	
	ErH_timer=0
    t_1sec = 0
    t_5sec = 0
	
    //restart_t=Time()
    

    While True
		
		ErH_timer = ErH_timer + 1
		       
    	//now_t = Time()
    	        //diff_t = now_t - start_t
    	//Thread.SetShareVar "diff_t", now_t - Thread.GetShareVar("start_t")
    	    	
        If Thread.getShareVar("FX_flag") = 1  and ReadUIConfig("FX")= True Then  //wait First "x"  AD cover appear.
       
        	If FPwaitTap(802, 10, 1075, 955, "Attachment:x.png", "000000", 0, 0.8, 10, 10, "EH -> waitFX") = True Then 
        		If FPRecheckTap(802, 10, 1075, 955, "Attachment:x.png", "000000", 0, 0.8, 10, 10, "EH -> checkFX") = True Then 
        	   		Thread.SetShareVar "FX_flag" , 0 
        	    	Delay 5000
        	    	Thread.SetShareVar "start_t", Time()
        		End If
        		
        	Else 
        		If FPwaitTap(779,183,1070,453,"Attachment:monpri.png","000000",0,0.8, 10, 10, "EH -> waitFX30ad") = True Then 
        			
        			Delay 5000	
        			Thread.SetShareVar "FX_flag", 0
        			KeyPress "Back"
        			ShowMessage "leave Fx check", 5000
        			Thread.SetShareVar "start_t", Time()  	    		
        		End If
        		
        		//Thread.SetShareVar "FX_flag" , 0	
        		
        	End If
        	
        	//Thread.SetShareVar "start_t", Time()
        	Call t_updateTM()
        	   	
             	
        Else 
        	   	
  
        	//Delay 5000//test
					
        	FindPic 89,685,995,1254,"Attachment:midy.png","000000",0,0.7,intX2,intY2    
        	FindPic 5, 3, 1073, 945, "Attachment:return.png", "000000", 0, 0.8, intX3, intY3
        	FindPic 287, 462, 780, 867, "Attachment:sysinfo.png", "000000", 0, 0.9, intX4, intY4
        	
        	If  intX3 > -1 And intY3 > -1 Then
            	
            	ShowMessage "EH -> return"
            	Tap intX3, intY3
            	Delay 5000					
  	
        	ElseIf intX2 > -1 And intY2 > -1 Then
        	
            	ShowMessage "EH -> midy"
            	Tap intX2, intY2
            	Delay 5000
        	ElseIf intX4 > -1 And intY4 > -1 Then
        	
            	ShowMessage "EH -> sys"
            	KeyPress "Back"
            		
        	End If
         	
                 	
        	If  Thread.getShareVar("mode") = "1" Then // robmode 		
       			
       			
            	//Thread.SetShareVar "diff_t", now_t - Thread.GetShareVar("robstart_t")
            	//diff_t = now_t - start_t
           	
            	FindPic 293, 10, 862, 111, "Attachment:giftMt.png", "000000", 0, 0.8, intX, intY        	
            	//FindStr 354,6,662,119, "gift", "FCFCFC-505050", 0.7, intX, intY
            	If intX > -1 And intY > -1 Then 
                	KeyPress "Back"
            	End If
        			
            	FindPic 107,810,969,1227, "Attachment:By.png", "000000", 0, 0.7, intX, intY
            	If intX > -1 And intY > -1 Then
                	Tap intX + 10, intY + 10
                	ShowMessage "EH -> bybattle" ,1000
            	End If 
        		
            	FindPic 785,1710,854,1751, "Attachment:s1.png", "000000", 0, 0.7, intX, intY
            	If intX > -1 And intY > -1 Then
                	t_1sec = 1
                	ShowMessage "EH -> 1sec" ,1000
            	End If
            	FindPic 785,1710,854,1751, "Attachment:s5.png", "000000", 0, 0.7, intX, intY
            	If intX > -1 And intY > -1 Then 
                	ShowMessage "EH -> 5sec" ,1000
                	t_5sec =1
            	End If
        		
            	If t_1sec = 1 Then 
                	ShowMessage "EH -> 1sec", 3000
                	Delay 2000		
            	End If
        		
            	If t_5sec = 1 Then 
                	ShowMessage "EH -> 5sec", 3000
                	Delay 2000		
            	End If
        		
        					
            	//Delay 5000//test	
        		    		
        		/*
        		
            	//Join faile handle					
            	FindPic 1, 153, 249, 940, "Attachment:rallyfull.png", "000000", 0, 0.9, intX2, intY2	
            	If intX2 > -1 And intY2 > -1 Then 
						
                	ShowMessage "EH -> Join faile"	
                	FindPic 0, 0, 273, 220, "Attachment:BattleBack.png", "000000", 0, 0.9, intX, intY
                	If intX > -1 And intY > -1 Then //exit battle	
                    	ShowMessage "EH ->  faile handling"	
                    	Tap intX + 10, intY + 10 //BattleBack
						
						
						Delay 5000
						FindPic 558, 1055, 951, 1182, "Attachment:y_exit_battle.png", "000000", 0, 0.9, intX, intY
						If intX > -1 And intY > -1 Then 
							Tap intX + 10, intY + 10
						End If
                	End If												
			
            	End If
            		
            	*/	
            End If
          
        End If
        
        
        
        
        
       	If  Thread.getShareVar("mode") = "1" Then // robmode 
       			
        	// exceed 10 min no action , restart app..        	
            If (Thread.getShareVar("diff_t") / 60) > 10 Then 
            
                FindPic 184, 17, 882, 116, "Attachment:rally.png", "000000", 0, 0.7, intX, intY	
                If intX > -1 Then //rally
            		
                    If t_1sec + t_5sec <> 2 Then 
                    	Call ResetTPWAR() 
                    End If
            		
                Else // not rally 
         			Call ResetTPWAR()
			
                End If
            	
                Thread.SetShareVar "start_t", Time()
                	
                t_1sec = 0
                t_5sec = 0        	
            End If
            
        Else //admode
        
        	If (Thread.GetShareVar ("diff_t") / 60) > 10 or  Thread.getShareVar( "gamedead_F") = 1 Then 
        	
        		//gamedead_F = False
        		Thread.setShareVar "gamedead_F", 0
        		
        		call ResetTPWAR()
    		End If
        		
        End If
        
        If ErH_timer mod 10 = 1 Then // time update about every 1min.
        	Call t_updateTM()
        	If ReadUIConfig("DBmsg") = True Then 
        		ShowMessage "EEH->diff_t:" & Thread.getShareVar("diff_t"), 3000
        	End If
            
          //ShowMessage "ErH_timer:" & ErH_timer, 3000
        End If       
       	
    Wend
    	
End Sub

Sub ResetTPWAR

	//now_t = Time()
	//robstart_t = Time()
	
	//start_t=Time()
	
	KillApp "com.topwar.gp"
	Delay 1000
	RunApp "com.topwar.gp"
	
	Thread.SetShareVar "FX_flag", 1
	
	ShowMessage "Restart topwar,delay 5 min..", 300000
	Delay 300000// for slow  machine
	Thread.SetShareVar "start_t", Time()
End Sub



